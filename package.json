{
  "name": "addigy",
  "version": "1.3.8",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -P tsconfig.build.json && cpy package.json dist && cpy README.md dist",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "open": "open",
    "qa": "run-s -l lint test build",
    "test": "echo 'no tests'",
    "test.cov": "run-s 'test --coverage' 'open coverage/lcov-report/index.html'",
    "test.watch": "jest --watchAll"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "addigy",
    "addigy api",
    "rest api",
    "rest"
  ],
  "author": "Caleb Albers <calebalbers@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/calebalbers/addigy-node"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/got": "^8.3.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.6",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^26.6.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "open": "^8.0.8",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.4.3",
    "typedoc": "^0.15.0",
    "typescript": "^4.2.3"
  },
  "volta": {
    "node": "14.16.0"
  }
}
